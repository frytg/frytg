				"weight": 50,
				"params": {
					"handle": "dan.frytg"
				}
			}
		]
	},
	"imaging": {
		"quality": 87
	},
	"minify": {
		"disableCSS": false,
		"disableHTML": false,
		"disableJS": false,
		"disableJSON": false,
		"disableSVG": false,
		"disableXML": false,
		"minifyOutput": false,
		"tdewolff": {
			"html": {
				"keepQuotes": true
			}
		}
	},
	"sitemap": {
		"changeFreq": "monthly",
		"disable": false,
		"filename": "sitemap.xml"
	},
	"taxonomies": {
		"category": "categories",
		"tag": "tags"
	},
	"build": {
		"buildStats": {
			"enable": true
		},
		"cachebusters": [
			{
				"source": "assets/watching/hugo_stats\\.json",
				"target": "styles\\.css"
			},
			{
				"source": "(postcss|tailwind)\\.config\\.js",
				"target": "css"
			},
			{
				"source": "assets/.*\\.(js|ts|jsx|tsx)",
				"target": "js"
			},
			{
				"source": "assets/.*\\.(.*)$",
				"target": "$1"
			}
		]
	},
	"outputs": {
		"home": ["html"],
		"section": ["html", "rss"],
		"taxonomy": ["html"],
		"term": ["html"]
	},
	"markup": {
		"goldmark": {
			"parser": {
				"attribute": {
					"block": true,
					"title": true
				}
			}
		}
	}
}
