{
  "title": "Daniel Freytag",
  "baseURL": "https://www.frytg.com/",
  "languageCode": "en-US",
  "params": {
    "contact": {
      "email": "info@example.org",
      "phone": "+1 202-555-1212"
    },
    "subtitle": "The Best Widgets on Earth"
  },
  "disableHugoGeneratorInject": true,
  "enableRobotsTXT": true,
  "menus": {
    "main": [
      {
        "name": "About me",
        "pageRef": "/",
        "weight": 10
      },
      {
        "name": "/social",
        "pageRef": "/social",
        "weight": 20
      },
      {
        "name": "/uses",
        "pageRef": "/uses",
        "weight": 30
      },
      {
        "name": "/blog",
        "pageRef": "/blog",
        "weight": 30
      }
    ]
  },
  "build": {
    "buildStats": {
      "enable": true
    },
    "cachebusters": [
      {
        "source": "assets/watching/hugo_stats\\.json",
        "target": "styles\\.css"
      },
      {
        "source": "(postcss|tailwind)\\.config\\.js",
        "target": "css"
      },
      {
        "source": "assets/.*\\.(js|ts|jsx|tsx)",
        "target": "js"
      },
      {
        "source": "assets/.*\\.(.*)$",
        "target": "$1"
      }
    ]
  },
  "minify": {
    "disableCSS": false,
    "disableHTML": false,
    "disableJS": false,
    "disableJSON": false,
    "disableSVG": false,
    "disableXML": false,
    "minifyOutput": false
  },
  "sitemap": {
    "changeFreq": "monthly"
  }
}
