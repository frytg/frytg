{
	"title": "Daniel Freytag",
	"baseURL": "https://www.frytg.com/",
	"languageCode": "en-US",
	"params": {
		"og_image": "images/og-card/about-2024-Q1-A-2x1.jpg"
	},
	"disableHugoGeneratorInject": true,
	"enableRobotsTXT": true,
	"menus": {
		"main": [
			{
				"name": "/about",
				"pageRef": "/",
				"weight": 10
			},
			{
				"name": "/social",
				"pageRef": "/social",
				"weight": 20
			},
			{
				"name": "/uses",
				"pageRef": "/uses",
				"weight": 30
			},
			{
				"name": "/blog",
				"pageRef": "/blog",
				"weight": 30
			}
		]
	},

	"imaging": {
		"quality": 85
	},
	"minify": {
		"disableCSS": false,
		"disableHTML": false,
		"disableJS": false,
		"disableJSON": false,
		"disableSVG": false,
		"disableXML": false,
		"minifyOutput": false,
		"tdewolff": {
			"html": {
				"keepQuotes": true
			}
		}
	},
	"sitemap": {
		"changeFreq": "monthly"
	},
	"build": {
		"buildStats": {
			"enable": true
		},
		"cachebusters": [
			{
				"source": "assets/watching/hugo_stats\\.json",
				"target": "styles\\.css"
			},
			{
				"source": "(postcss|tailwind)\\.config\\.js",
				"target": "css"
			},
			{
				"source": "assets/.*\\.(js|ts|jsx|tsx)",
				"target": "js"
			},
			{
				"source": "assets/.*\\.(.*)$",
				"target": "$1"
			}
		]
	},
	"markup": {
		"goldmark": {
			"parser": {
				"attribute": {
					"block": true,
					"title": true
				}
			}
		}
	}
}
